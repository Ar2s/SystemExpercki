rule "Bulbasaur"
    when
    	w : Wyswietlanie();
    	Cecha(tresc == "Nogi4");
    	Cecha(tresc == "NogiKoloruZielonego");
    	Cecha(tresc == "Oczy2");
    	Cecha(tresc == "OczyKoloruCzerwonego");
    	Cecha(tresc == "Wzor");
    	Cecha(tresc == "Pazury");
    	Cecha(tresc == "Zeby");
    	Cecha(tresc == "UszyKoloruZielonego");
    	Cecha(tresc == "Nosi");
    then
    	w.set("Bulbasaur", true);        
    	retract(w);
end

rule "Charizard"
	when
		w : Wyswietlanie();
    	Cecha(tresc == "Nogi2");
    	Cecha(tresc == "NogiKoloruPomaranczowego");
    	Cecha(tresc == "Oczy2");
    	Cecha(tresc == "Pazury");
    	Cecha(tresc == "Zeby");
		Cecha(tresc == "RogiKoloruPomaranczowego");
		(or Cecha(tresc == "OgonKoloruPomaranczowego");
			Cecha(tresc == "OgonKoloruKremowego");
		)
		(or Cecha(tresc == "SkrzydlaKoloruPomaranczowego");
			Cecha(tresc == "SkrzydlaKoloruNiebieskiego");
		)
		Cecha(tresc == "ReceKoloruPomaranczowego");
    then
    	w.set("Charizard", true);        
    	retract(w);
end

