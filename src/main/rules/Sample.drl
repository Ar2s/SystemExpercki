rule "Nogi" salience 2
    when
    	w : Wyswietlanie();
    	not Pytanie(tekst == "Nogi");
    then
    	insert(new Pytanie("Nogi"));
        w.set("Czy twoj pokemon ma nogi?", "Nogi", FormatOdp.YESNO);
    	retract(w);
end

